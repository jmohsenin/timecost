{
    "manifest_version": 2,
    "name": "Timecost for Google Calendar",
    "short_name": "Timecost",
    "version": "0.1.1",
    "content_scripts": [
        {
            "matches": [
                "https://calendar.google.com/*",
                "http://calendar.google.com/*"
            ],
            "js": [
                  "jquery-3.1.1.slim.min.js",
                  "moment-with-locales.js",
                  "content.js"
            ]
        }
    ],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
