{
    "manifest_version": 2,
    "name": "Timecost",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "https://calendar.google.com/*",
                "http://calendar.google.com/*"
            ],
            "js": [
                  "jquery-3.1.1.slim.min.js",
                  "moment-with-locales.js",
                  "content.js"
            ]
        }
    ],
    "options_page": "options.html",
    "permissions": [
        "storage"
    ]
}
